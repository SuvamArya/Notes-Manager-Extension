{
    "manifest_version": 3,
    "name": "Chrome To-Do",
    "description": "A simple and clean to-do list extension! ",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png"
    }
}
